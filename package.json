  },
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "circular-json": "^0.5.1",
    "classnames": "^2.2.6",
    "connect-session-sequelize": "^5.2.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "ganache-cli": "^6.1.0",
    "history": "^4.7.2",
    "install": "^0.11.0",
    "material-ui": "^1.0.0-beta.47",
